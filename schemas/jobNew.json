{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "title",
        "salary",
        "equity",
        "company_handle"
    ],
    "properties": {
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "Information officer"
            ]
        },
        "salary": {
            "type": "integer",
            "default": 0,
            "title": "The salary Schema",
            "examples": [
                200000
            ]
        },
        "equity": {
            "type": "Numeric",
            "default": 0,
            "title": "The equity Schema",
            "examples": [
                0
            ]
        },
        "company_handle": {
            "type": "string",
            "default": "",
            "title": "The company_handle Schema",
            "examples": [
                "hall-mills"
            ]
        }
    },
    "examples": [{
        "title": "Information officer",
        "salary": 200000,
        "equity": 0,
        "company_handle": "hall-mills"
    }]
}